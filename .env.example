# 爬虫服务配置
# 页面池最大大小（默认8个页面）
CRAWLER_MAX_PAGES=8

# Excel批量处理配置
# 最大并发线程数（默认6个线程）
EXCEL_MAX_WORKERS=6

# Supabase配置
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# 前端环境变量
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# 性能调优建议：
# - 低配置电脑：CRAWLER_MAX_PAGES=4, EXCEL_MAX_WORKERS=3
# - 中等配置电脑：CRAWLER_MAX_PAGES=8, EXCEL_MAX_WORKERS=6
# - 高配置电脑：CRAWLER_MAX_PAGES=12, EXCEL_MAX_WORKERS=10